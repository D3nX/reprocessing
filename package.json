{
  "name": "Reprocessing",
  "version": "0.1.0",
  "description": "Processing library for Reason",
  "scripts": {
    "build": "bsb -make-world -backend js",
    "build:bytecode": "bsb -make-world -backend bytecode",
    "build:native": "bsb -make-world -backend native",
    "buildweb": "bsb -make-world -backend js && webpack -d lib/js/src/index.js lib/js/src/app.js"
  },
  "dependencies": {
    "ReasonglInterface": "github:bsansouci/reasongl-interface",
    "ReasonglNative": "github:bsansouci/reasongl-native",
    "ReasonglWeb": "github:bsansouci/reasongl-web",
    "matchenv": "bsansouci/matchenv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsansouci/reprocessing.git"
  },
  "author": "bsansouci & schmavery",
  "devDependencies": {
    "browserify": "*",
    "bs-platform": "bsansouci/bsb-native"
  }
}
